	.init.text : AT(0xC000) 
	{ 
		*(.init)
	}
	.init.data :
	{
		*(.init.data)
	}

	.usb_ram 0xC000D000 : AT(0xD000) 
	{
		*(.usb_ram)
	}
	
	.text 0xC000F000 : AT(0xF000) 
	{ 
		*(.text)
	}
	
	__data_start__ = .;
	
	.data : 
