	.exception_handler_map_table 0xC0000000 : AT(0x0000) 
	{
		*(.exception_handler_map_table)
	}
	
	. = 0x1000;
	
	.exception_handler_table 0xC0001000 : AT(0x1000) 
	{
		*(.exception_handler_table)
	}	
	
