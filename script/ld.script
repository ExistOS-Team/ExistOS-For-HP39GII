	{
		*(.kernel_heap_memory)
	}





	. = 0xC0003000;
	
	__VRAM_BASE = .;
	
	
	/* . = 0xC000A910;		 === 0x9910 === VRAM ==*/
	
	
	
	. = 0xC000C000;
	ABT_STACK_ADDR = .;
	
	. = . + 0x300;
