Lpoly(n):={
  local LP,j,P,S;
  P:=normal(isopolygon(0,1,n));
  S:=segment(0,evalf(1/2+i/2/tan(pi/n)));
  LP:=[[P,S]];
  for (j:=0;j<2*n;j++){
    LP:=concat(LP,[rotation(j+1,evalf(-pi/n),LP[2*j]),rotation(j+1,evalf(-2*pi/n),LP[2*j])]);
  }
  return LP;
}:;,
